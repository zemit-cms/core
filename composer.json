  "keywords": [
    "framework",
    "phalcon",
    "cms",
    "core",
    "zemit",
    "zemit-cms",
    "zemit cms",
    "zemit cms core"
  ],
  "homepage": "https://www.zemit.com",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Zemit Team",
      "email": "contact@zemit.com",
      "homepage": "https://www.zemit.com/"
    },
    {
      "name": "Nuagerie Team",
      "email": "contact@nuagerie.com",
      "homepage": "https://www.nuagerie.com/"
    },
    {
      "name": "Julien Turbide",
      "email": "contact@jturbide.com",
