    {
      "name": "Contributors",
      "homepage": "https://github.com/zemit-cms/core/graphs/contributors"
    }
  ],
  "funding": [
    {
      "type": "github",
      "url": "https://github.com/sponsors/jturbide"
    }
  ],
  "support": {
    "issues": "https://github.com/zemit-cms/core/issues",
    "source": "https://github.com/zemit-cms/core"
  },
  "config": {
    "optimize-autoloader": true,
    "apcu-autoloader": true,
    "apcu-autoloader-prefix": "zemit-core-",
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "funkjedi/composer-include-files": false
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=8.2",
    "ext-intl": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "ext-pdo": "*",
    "ext-phalcon": ">=5.7",
    "ext-sodium": "*",
    "docopt/docopt": "^1.0.5",
    "league/flysystem": "^3.29.1",
    "league/fractal": "^0.20.2",
    "phalcon/devtools": "~5.0",
    "phalcon/incubator-mailer": "^2.0",
    "vlucas/phpdotenv": "^5.6.1"
  },
  "suggest": {
    "ext-zend-opcache": "*",
    "ext-apcu": "*",
    "ext-bcmath": "*",
    "ext-gd": "*",
    "ext-redis": "*",
    "ext-simplexml": "*",
    "ext-imap": "*",
    "ext-sockets": "*",
    "google/recaptcha": "^1.3",
    "aws/aws-sdk-php": "^3.300",
    "mustache/mustache": "^2.14",
    "smarty/smarty": "^4.4",
    "twig/twig": "^3.8",
    "xenolope/quahog": "^3.0",
    "php-imap/php-imap": "^5.0",
    "orhanerday/open-ai": "^5.1",
    "joshtronic/php-loremipsum": "^2.1",
    "league/oauth2-client": "^2.7",
    "league/oauth2-google": "^4.0",
    "league/oauth2-facebook": "^2.2",
    "league/oauth2-linkedin": "^5.1",
    "league/oauth2-instagram": "^3.1",
    "league/oauth2-github": "^3.1",
    "thiagoalessio/tesseract_ocr": "^2.13",
    "shuchkin/simplexlsxgen": "^1.4",
    "league/csv": "^9.16",
    "spatie/array-to-xml": "^3.2"
  },
  "require-dev": {
    "aws/aws-sdk-php": "^3.337.3",
    "google/recaptcha": "^1.3.0",
    "joshtronic/php-loremipsum": "^2.1.0",
    "league/csv": "^9.21.0",
    "league/oauth2-client": "^2.8.0",
    "league/oauth2-facebook": "^2.2.0",
    "league/oauth2-github": "^3.1.1",
    "league/oauth2-google": "^4.0.1",
    "league/oauth2-instagram": "^3.1.0",
    "league/oauth2-linkedin": "^5.1.2",
    "mustache/mustache": "^2.14.2",
    "orhanerday/open-ai": "^5.2",
    "pds/skeleton": "^1.0.0",
    "phalcon/ide-stubs": "^5.8.0",
    "php-imap/php-imap": "^5.0.1",
    "phpspec/phpspec": "^7.5.0",
    "phpstan/phpstan": "^1.12.16",
    "phpunit/phpunit": "^10.5.41",
    "shuchkin/simplexlsxgen": "^1.4.13",
    "smarty/smarty": "^4.5.5",
    "spatie/array-to-xml": "^3.4.0",
    "squizlabs/php_codesniffer": "^3.11.3",
    "thiagoalessio/tesseract_ocr": "^2.13.0",
    "twig/twig": "^3.18.0",
    "xenolope/quahog": "^3.0.0"
  },
  "autoload": {
    "psr-4": {
      "Zemit\\": "src/"
    },
    "files": [
      "src/Functions/Dump.php",
      "src/Functions/Array.php",
      "src/Functions/Sprintf.php",
      "src/Functions/Json.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Zemit\\Tests\\Unit\\": "tests/Unit"
    }
  },
  "bin": [
    "zemit"
  ]
}
